                    <li><a><i class="icon-home"></i> القائمه</a></li>
                    <li class="active"><a style="color: #ec407a">بيانات العملاء</a> </li>
                </ol>

                <div class="panel panel-default">
                    <div class="panel-heading">
                        <strong style="color: #5c4f4f">بيانات العملاء</strong>

                        <i class="fa fa-plus-circle text-info btn-lg" data-toggle="modal" data-target="#addClient" style="font-size: 25px; color: #5c4f4f; cursor:pointer">
                        </i>
                    </div>
                    <div class="row w3-res-tb">
                        <div class="col-lg-12" style="margin-left:8%">

                            <div class="row" dir="rtl">
                                <div class="col-lg-2">
                                    <button class="btn" style="border-radius: 0px; width:138px" data-toggle="modal" data-target="#get_client">العميل الرئيسي</button>

                                </div>
                                <div class="col-lg-2">
                                    <input id="clientName" ng-model="pName2" ng-value=" " type="text" class="form-control" style="border-radius:0px ; background:white" placeholder=" اسم العميل الرئيسي" />

                                </div>
                                <div class="col-lg-2">
                                    <select ng-model="token.areaName" class="form-control" style="  border-radius: 0px;">
                                        <option value="">جميع المناطق</option>
                                        <option ng-repeat="x1 in allArea | filter: token.govern">{{x1.name}}</option>
                                    </select>
                                </div>
                                <div class="col-lg-2">
                                    <select ng-model="token.govern" class="form-control" style="  border-radius: 0px;">
                                        <option value=""> المحافظه</option>
                                        <option ng-repeat="x2 in allGovern">{{x2.name}}</option>
                                    </select>
                                </div>
                                <div class="col-lg-2">
                                    <select ng-model="token.type" class="form-control" style="  border-radius: 0px;">
                                        <option value="">نوع العميل</option>
                                        <option ng-repeat="x2 in allTypes">{{x2.type}}</option>
                                    </select>
                                </div>
                            </div>
                            <br />
                            <div class="row" dir="rtl">
                                <div class="col-lg-2">
                                    <input type="text" ng-model="token.id_num" placeholder="الرقم القومي" class="form-control" style="font-size: 12px; border-radius: 0px;" />

                                </div>
                                <div class="col-lg-2">
                                    <input type="text" ng-model="token.phones.number" placeholder="رقم التليفون" class="form-control" style="font-size: 12px; border-radius: 0px;" />
                                </div>
                                <div class="col-lg-2">
                                    <input type="text" placeholder="اسم العميل" class="form-control" style="font-size: 12px; border-radius: 0px;" ng-model="token.name" />
                                </div>
                                <div class="col-lg-2">
                                    <select ng-model="token.delegatorId" class="selectpicker form-control" dir="rtl" style="border-radius:0px">
                                        <option value="">  مندوب المبيعات </option>
                                        <option ng-repeat="x2 in allDelegators | filter:agentsS" ng-value="{{x2.id}}">{{x2.name}}</option>
                                    </select>
                                </div>
                                <div class="col-lg-2">
                                    <select ng-model="agentsS" class="selectpicker form-control" dir="rtl" style="border-radius:0px">
                                        <option value="">  الوكيل </option>
                                        <option ng-repeat="x2 in allAgents">{{x2.name}}</option>
                                    </select>
                                </div>
                            </div>


                            <br />
                            <div class="row" dir="rtl">
                                <div class="col-lg-2">
                                    <select ng-model="token.phones.name" class="form-control" style="  border-radius: 0px;">
                                        <option value="">الشبكه</option>
                                        <option ng-repeat="x2 in kroot">{{x2.name}}</option>
                                    </select>
                                </div>
                                <div class="col-lg-2">
                                    <select ng-model="token.available" class="form-control" style="  border-radius: 0px;">
                                        <option value="">حاله العميل</option>
                                        <option value="1">متاح</option>
                                        <option value="0">غير متاح</option>
                                    </select>
                                </div>
                                <div class="col-lg-2">
                                    <select ng-model="token.activation_date" class="form-control" style="  border-radius: 0px;">
                                        <option value="">حاله العقد</option>
                                        <option value="/">مفعل</option>
                                        <option value="لم">غير مفعل</option>
                                    </select>
                                </div>
                                <div class="col-lg-2 ">

                                    <input ng-model="end" type="number" class="form-control" style="border-radius: 0px;" placeholder="الي العميل رقم" />

                                </div>

                                <div class="col-lg-2 ">

                                    <input ng-model="start" type="number" class="form-control" style="border-radius: 0px;" placeholder="من العميل رقم" />

                                </div>
                            </div>


                            <br />
                            <div class="row" dir="rtl">

                                <div class="col-lg-2">
                                    <select ng-model="token.done" class="form-control" style="  border-radius: 0px;">
                                        <option value="">الكل</option>
                                        <option value="1">تم التعاقد</option>
                                        <option value="0">لم يتم التعاقد</option>
                                    </select>
                                </div>

                                <div class="col-lg-2">

                                    <input ng-model="to1" placeholder="الي تاريخ تفعيل" style="border-radius: 0px;" class="form-control" type="text" onfocus="(this.type='date')">

                                </div>

                                <div class="col-lg-2 ">

                                    <input ng-model="from1" placeholder="من تاريخ تفعيل" style="border-radius: 0px;" class="form-control" type="text" onfocus="(this.type='date')">

                                </div>
                                <div class="col-lg-2 ">


                                    <input ng-model="to" placeholder="الي تاريخ تعاقد"
                                           style="border-radius: 0px;" class="form-control" type="text" onfocus="(this.type='date')">


                                </div>

                                <div class="col-lg-2 ">

                                    <input id="datepicker" ng-model="from" placeholder="من تاريخ تعاقد"
                                           style="border-radius: 0px;" class="form-control" type="text" onfocus="(this.type='date')">

                                </div>
                            </div>

                            <div class="row" dir="rtl">
                                <div class="col-lg-2 col-lg-offset-5">
                                    <br />
                                    <button class="btn" style="border-radius: 0px; width: 310px;">
                                        بحث
                                    </button>
                                </div>
                            </div>


                        </div>
                    </div>

                    <br />
                    <div class="table-responsive">

