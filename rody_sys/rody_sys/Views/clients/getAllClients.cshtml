                        <input type="text" ng-model="phNum" class="form-control" placeholder="رقم التليفون" style="width:300px" />

                        <br />
                        <div class="table-responsive" style="height:300px; overflow:auto">

                            <table class="table table-striped b-t b-light" dir="rtl">
                                <thead>
                                    <tr style="background:#FAFAFA; ">
                                        <th><h3 style="padding:2px;"> الكود </h3></th>
                                        <th><h3 style="padding:2px;"> رقم التليفون  </h3></th>
                                        <th><h3 style="padding:2px;"> الشريحه </h3></th>
                                        <th><h3 style="padding:2px;"> تعديل </h3></th>
                                        <th><h3 style="padding:2px;"> حذف </h3></th>
                                    </tr>
                                </thead>
                                <tbody ng-repeat="c in allClientPhones | filter:{number:phNum}">

                                    <tr style="background:white">

                                        <td><h4 style="font-size:11px">{{$index + 1}}</h4></td>
                                        <td><input id="ph{{$index}}" dir="ltr" type="text" value="{{c.number}}" class="form-control" /></td>
                                        <td>
                                            <select id="cart{{$index}}" class="selectpicker form-control" dir="rtl" style="border-radius:0px">
                                                <option value="">الشريحه</option>
                                                <option ng-repeat="x2 in kroot" ng-value="{{x2.id}}" ng-selected="{{x2.id==c.sharehaId}}">{{x2.name}}</option>
                                            </select>
                                        </td>
                                        <td>
                                            <button class="btn btn-success btn-sm" ng-click="updatePhone(c.id , $index )" type="button" data-dismiss="modal">
                                                <i class="fa fa-edit "></i>
                                            </button>
                                        </td>
                                        <td>
                                            <button class="btn btn-danger btn-sm" ng-click="deletePhone(c.id)" type="button" data-dismiss="modal">
                                                <i class="fa fa-remove "></i>
                                            </button>
                                        </td>
                                    </tr>
                                </tbody>


                                <tbody></tbody>
                            </table>

                        </div>
                        <br />
                        <center>
                            <button type="button" class="btn btn-default" data-dismiss="modal" style="width:100px"><h4>الغاء</h4></button>
                        </center>

                    </center>


                </div>

            </div>
        </div>
    </div>
    <!--====================================================================================-->

    <div id="add_new_phone" class="modal fade left" role="dialog" style="top:10% ;z-index:9998">
        <div class=" modal-dialog" style="width:400px">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="text-center" style="color: #5c4f4f">اضافه رقم جديد</h4>
                </div>
                <div class="modal-body text-left">
                    <center>
                        <br /><br />
                        <div class="col-sm-12">
                            <div class="form-group">
                                <input ng-model="adPhone" type="text" class="form-control" placeholder="رقم التليفون">
                            </div>
                        </div>
                        <div class="col-sm-12">
                            <div class="form-group">
                                <select ng-model="adShareha" class="selectpicker form-control" dir="rtl" style="border-radius:0px">
                                    <option value="">الشريحه</option>
                                    <option ng-repeat="x2 in kroot" ng-value="{{x2.id}}">{{x2.name}}</option>
                                </select>
                            </div>
                        </div>


                        <center>
                            <br /><br />
                            <button type="button" class="btn btn-info" data-dismiss="modal" style="width:100px; margin-top:20px">الغاء</button>
                            <button type="button" class="btn btn-danger" data-dismiss="modal" style="width:100px; margin-top:20px" ng-click="addPhone()">اضافه</button>
                        </center>

                    </center>


                </div>

            </div>
        </div>
    </div>

